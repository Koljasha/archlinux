# Host __hostname__
  # User __username__
  # Hostname __hostname__
  # IdentityFile ~/.ssh/__key__

# Github.com
Host github.com
  PreferredAuthentications publickey
  IdentityFile ~/.ssh/git_rsa

# Bitbucket.org
Host bitbucket.org
  PreferredAuthentications publickey
  HostkeyAlgorithms +ssh-rsa
  PubkeyAcceptedAlgorithms +ssh-rsa
  IdentityFile ~/.ssh/git_rsa

# GitLab.com
Host gitlab.com
  PreferredAuthentications publickey
  IdentityFile ~/.ssh/git_rsa
